---
format: xml
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Project Blacklight</title>
 <link href="http://projectblacklight.github.com/atom.xml" rel="self"/>
 <link href="http://projectblacklight.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://projectblacklight.org</id>
 <author>
   <name>Project Blacklight</name>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://projectblacklight.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>id:{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
